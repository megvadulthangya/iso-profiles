{
   "action": {
      "default_icon": "img/icon16.png",
      "default_popup": "popup.html",
      "default_title": "Tab Suspender"
   },
   "background": {
      "service_worker": "worker.js"
   },
   "commands": {
      "add-to-white-list": {
         "description": "Add to Whitelist..."
      },
      "ignore-current-tab": {
         "description": "Ignore Current Tab"
      },
      "remove-current-tab-from-ignore": {
         "description": "Remove Current Tab From Ignore"
      },
      "remove-from-white-list": {
         "description": "Remove from Whitelist"
      },
      "suspend-all-other": {
         "description": "Suspend All Other Tabs",
         "suggested_key": {
            "chromeos": "Alt+O",
            "default": "Alt+O",
            "linux": "Alt+O",
            "mac": "Command+Shift+O",
            "windows": "Alt+O"
         }
      },
      "suspend-all-window": {
         "description": "Suspend All Window",
         "suggested_key": {
            "chromeos": "Alt+W",
            "default": "Alt+W",
            "linux": "Alt+W",
            "mac": "Command+Shift+W",
            "windows": "Alt+W"
         }
      },
      "suspend-current": {
         "description": "Suspend Current Tab",
         "suggested_key": {
            "chromeos": "Alt+S",
            "default": "Alt+S",
            "linux": "Alt+S",
            "mac": "Command+Shift+S",
            "windows": "Alt+S"
         }
      },
      "suspend-or-unsuspend-current-tab": {
         "description": "Suspend or Unsuspend Current Tab (in one HotKey)"
      },
      "unsuspend-all-tabs": {
         "description": "Unsuspend All Tabs"
      },
      "unsuspend-current-tab": {
         "description": "Unsuspend Currnet Tab"
      },
      "unsuspend-current-window": {
         "description": "Unsuspend Current Window",
         "suggested_key": {
            "chromeos": "Alt+U",
            "default": "Alt+U",
            "linux": "Alt+U",
            "mac": "Command+Shift+U",
            "windows": "Alt+U"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "lib/h2c.js", "inject.js" ],
      "match_origin_as_fallback": true,
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
   },
   "default_locale": "en",
   "description": "__MSG_extDescription__",
   "host_permissions": [ "*://*/*", "\u003Call_urls>" ],
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "64": "img/icon64.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 3,
   "minimum_chrome_version": "116",
   "name": "__MSG_extName__",
   "offline_enabled": true,
   "optional_permissions": [  ],
   "options_page": "fancy-settings/source/index.html",
   "permissions": [ "tabs", "notifications", "unlimitedStorage", "contextMenus", "storage", "scripting", "favicon", "offscreen" ],
   "short_name": "Tab Suspender",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0.7.2",
   "web_accessible_resources": [ {
      "extension_ids": [ "*" ],
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "_favicon/*", "fancy-settings/source/index.html", "*.html", "*.js", "*.js.map", "*.css", "/img/*.png", "img/*.ico", "video/*.mp4" ]
   } ]
}
